# MaxMind Hub Environment Variables - Managed by Ansible
# This file contains sensitive credentials

# MaxMind API credentials
MAXMIND_LICENSE_KEY={{ vault_maxmind_license_key }}
MAXMIND_ACCOUNT_ID={{ vault_maxmind_account_id }}

# Hub authentication tokens
HUB_TOKEN_DEV={{ vault_hub_token_dev }}
HUB_TOKEN_STAGING={{ vault_hub_token_staging }}
HUB_TOKEN_PROD={{ vault_hub_token_prod }}

# Storage configuration
MAXMIND_HUB_DATA_DIR=/data/maxmind

# Server port
PORT=8443
